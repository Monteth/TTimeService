{
  "name": "ttime-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "node dist/bin/www",
    "transpile": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run transpile",
    "dev": "npm run build && npm run server",
    "prod": "npm run build && npm run server",
    "watch:dev": "nodemon"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "bcrypt": "^4.0.1",
    "config": "^3.3.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.10",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.9.0"
  }
}
